<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/select_date" android:layout_marginBottom="11dip" android:layout_marginLeft="11dip" android:layout_marginTop="11dip"></TextView>
    <TimePicker android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/timePicker" android:layout_gravity="center_horizontal"></TimePicker>
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/select_intensity" android:layout_marginLeft="11dip" android:layout_marginBottom="11dip" android:layout_marginTop="22dip"></TextView>
        <SeekBar android:layout_weight="1" android:layout_height="wrap_content" android:layout_width="match_parent" android:layout_marginLeft="11dip" android:layout_marginRight="11dip" android:id="@+id/brightControl" android:max="255"></SeekBar>
        <TextView android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="22dip" android:layout_marginBottom="11dip" android:layout_marginLeft="11dip" android:text="@string/select_effect"></TextView>
        <ToggleButton android:layout_height="70dip" android:layout_width="70dip" android:layout_gravity="center_horizontal" android:id="@+id/organicBreathingButton"></ToggleButton>
    
</LinearLayout>
